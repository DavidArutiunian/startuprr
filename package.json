    "babel-preset-env": "^1.6.1",
    "bootstrap-sass": "^3.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "extract-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
