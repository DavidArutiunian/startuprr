{
  "name": "startuprr",
  "version": "1.0.0",
  "description": "frontend course project",
  "engines": {
    "node": "^8.10.0",
    "npm": "^5.8.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "build": "cross-env NODE_ENV=production webpack --progress -p --config webpack.config.build.js",
    "postinstall": "if-env NODE_ENV=production && npm run build || ls -l",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config webpack.config.dev.js"
  },
  "keywords": [
    "frontend",
    "course",
    "html",
    "landing"
  ],
